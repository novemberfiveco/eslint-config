{
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/novemberfiveco/eslint-config.git"
  },
  "engines": {
    "node": ">=22.14",
    "npm": ">= 10",
    "yarn": "please-use-npm"
  },
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "release:typescript": "npm run release --workspace=@novemberfiveco/eslint-config-typescript",
    "release:next": "npm run release --workspace=@novemberfiveco/eslint-config-next",
    "release:vite": "npm run release --workspace=@novemberfiveco/eslint-config-vite",
    "release:serverless": "npm run release --workspace=@novemberfiveco/eslint-config-serverless",
    "release:react-native": "npm run release --workspace=@novemberfiveco/eslint-config-react-native"
  }
}
