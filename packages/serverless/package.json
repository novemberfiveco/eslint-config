{
  "name": "@novemberfiveco/eslint-config-serverless",
  "version": "0.2.0",
  "private": false,
  "description": "ESLint configuration for Serverless projects used by November Five",
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript"
  ],
  "main": "index.js",
  "homepage": "https://github.com/novemberfiveco/eslint-config#readme",
  "author": "Min Jie Fan <minjie@novemberfive.co>",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "@rushstack/eslint-patch": "1.3.3",
    "@serverless/eslint-config": "5.1.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "release-it": "^15.9.3"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "prettier": ">=3.0.3",
    "typescript": ">=5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "eslint-config-serverless@${version}"
    },
    "github": {
      "release": true,
      "autoGenerate": true,
      "releaseName": "esling-config-serverless@${version}",
      "tokenRef": "GITHUB_TOKEN"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "perf",
              "section": "Performance improvements"
            }
          ]
        },
        "infile": "CHANGELOG.md",
        "ignoreRecommendedBump": true
      }
    }
  }
}
