{
  "name": "@novemberfiveco/eslint-config-serverless",
  "version": "0.1.0",
  "private": false,
  "description": "ESLint configuration for Serverless projects used by November Five",
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript"
  ],
  "main": "index.js",
  "homepage": "https://github.com/novemberfiveco/eslint-config#readme",
  "author": "Min Jie Fan <minjie@novemberfive.co>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "release-it": "^15.9.3"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "release": "release-it"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "eslint-config-serverless@${version}"
    },
    "github": {
      "release": true,
      "autoGenerate": true,
      "releaseName": "esling-config-serverless@${version}",
      "tokenRef": "GITHUB_TOKEN"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "perf",
              "section": "Performance improvements"
            }
          ]
        },
        "infile": "CHANGELOG.md",
        "ignoreRecommendedBump": true
      }
    }
  }
}
